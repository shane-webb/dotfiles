krakenssh () {
    ssh -o "UserKnownHostsFile /dev/null" -o "StrictHostKeyChecking=no" kraken-dev-$1 -t sudo su - sofi;
}

git-update-target-branch() {
    target_branch=$1

    git stash
    git checkout $target_branch
    git pull
}
alias update=git-update-target-branch

reverse-tunnel-app-via-kube() {
  kraken_box_number=$1
  application_name=$2
  port_number=$3

  if [[ ! $kraken_box_number || ! $application_name ]]; then
    echo "Must provide a dev box number and the name of an application."
    return
  fi

  echo "Tunneling $application_name to dev box $kraken_box_number."
  cd ~/git/sofi-kubernetes/scripts
  update master
  ./kraken-register-service.sh kraken-dev-$kraken_box_number.sofitest.com $application_name $port_number
}
alias tunnel=reverse-tunnel-app-via-kube

